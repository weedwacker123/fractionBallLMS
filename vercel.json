{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://fractionball-backend-110595744029.us-central1.run.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
